{
  "name": "vue-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "server": "npm run build && gulp && ts-node-dev ./_server/src/app.ts",
    "lint-staged": "eslint src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint --ext .js",
    "**/*.vue": "eslint --ext .vue"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "less": "^4.0.0",
    "less-loader": "^7.2.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.2",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  }
}
